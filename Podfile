# Uncomment the next line to define a global platform for your project
platform :ios, '12.0'

target 'BackendDataExchange' do
  # As of CocoaPods 1.5.0, you can use CocoaPods with static libraries! This is a big deal,since not all libraries you might depend on are shipped as frameworks. By omitting use_frameworks! from your Podfile, CocoaPods will integrate into your app using static libraries instead. 
  # use_frameworks!

  # Pods for BackendDataExchange
  pod 'Alamofire', '~> 4.7'
  pod 'MBProgressHUD', '~> 1.1.0', :modular_headers => true

end
